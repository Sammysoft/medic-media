{{> nav-body}}


<section class="content">
    <div class="container-fluid">
        <div class="block-header">
            <h2>All Physicians</h2>
            <small class="text-muted">Welcome to Medic Media</small>
        </div>
        <div class="row clearfix">
            {{#each Physicians}}
            <div class="col-xl-3 col-lg-4 col-md-6 col-sm-12">
                <div class="card">
                    <div class="body">
                        <div class="member-card verified">                            
                            <div class="thumb-xl member-thumb">
                                <img src="{{#each this}}{{this.imageurl}}{{/each }}" class="img-thumbnail rounded-circle" alt="profile-image">                               
                            </div>

                            <div class="">
                                <h4 class="m-b-5 m-t-20">Dr. {{#each this}}{{this.username}}{{/each }}</h4>
                            </div>

                            <p class="text-muted">{{#each this}}{{this.phonenumber}}{{/each }}</p>                           
                            <a href="/profile{{#each this}}/{{this._id}}"{{/each }}  class="btn btn-raised btn-sm">View Profile</a><br/><a href={{#each this}}"/delete/{{this._id}}"{{/each }} onclick="return confirm('Are you sure to delete this record?...')" class="edit"><i class="zmdi zmdi-delete"></i></a>
                           
                        </div>
                    </div>
                </div>
              
            </div>
              {{/each }}


</div>
</div>
</section>

